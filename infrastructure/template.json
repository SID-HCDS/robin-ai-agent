      "location": "[parameters('region')]",
      "properties": {
        // <-- COPY THE CORRESPONDING "properties" FROM YOUR ORIGINAL TEMPLATE HERE -->
      }
    },
    {
      "type": "Microsoft.CognitiveServices/accounts",
